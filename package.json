{
  "name": "vtur",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,vue,md,json,yaml,yml,css,scss,html}\"",
    "check-format": "prettier --check \"**/*.{ts,tsx,js,jsx,vue,md,json,yaml,yml,css,scss,html}\""
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.0",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
